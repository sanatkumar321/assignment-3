#include <iostream>
using namespace std;
int main()
{
	int n,i,isprime,j;
	cout << "enter a positive integer : ";
	cin>>n;
	cout << "factors of "<<n<<" are : " << endl;
	for (i=1;i<=n;++i)
	{
	   if(n%i==0)
	  {
	    for (j= 2;j<=i/2 ; j++)
	   {
	  	isprime=1;
		if (n%i==0)
		{
			isprime = 0 ;
			break;
			
		}
	    }
	    if (isprime =1)
	    cout<< i<<endl;	
  	   
	   }
	  
	}
	return 0;
}
